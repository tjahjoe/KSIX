        <jsScript_script>//Script here

// Ambil tanggal dari input stream (di list kiri namanya orderDate)
var src = orderDate;

if (src == null) {
  time_id = 0;
  month_name = "";
  weekday = "";
} else {
  // convert java.util.Date -> JS Date
  var d = new Date(src.getTime());

  // ambil komponen tanggal
  var y = d.getFullYear();
  var m = d.getMonth() + 1;  // 1-12
  var dd = d.getDate();      // 1-31
  var wd = d.getDay();       // 0=Sun ... 6=Sat

  // time_id format YYYYMMDD (Integer)
  time_id = (y * 10000) + (m * 100) + dd;

  // nama bulan
  var monthNames = [
    "January","February","March","April","May","June",
    "July","August","September","October","November","December"
  ];
  month_name = monthNames[m - 1];

  // nama hari
  var weekdayNames = ["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"];
  weekday = weekdayNames[wd];
}</jsScript_script>
